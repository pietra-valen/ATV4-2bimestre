programa
{
	
	funcao inicio()
	{
		inteiro num1, num2, num3, num4, num5, soma, media

        escreva("Informe um número: ")
        leia(num1)

        escreva("Informe outro número: ")
        leia(num2)

        escreva("Informe outro número: ")
        leia(num3)

        escreva("Informe outro número: ")
        leia(num4)

        escreva("Informe outro número: ")
        leia(num5)

        soma = num1 + num2 + num3 + num4 + num5
        media = soma/5

        escreva("A soma dos números digitados é: ", soma, " e a média é: ", media)
	}
}
